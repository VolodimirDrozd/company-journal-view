<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
	<display-name>company-journal</display-name>
	<servlet>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>com.testproject.controller.HomeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserReadServlet</servlet-name>
		<servlet-class>com.testproject.controller.user.UserReadServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserCreateServlet</servlet-name>
		<servlet-class>com.testproject.controller.user.UserCreateServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserUpdateServlet</servlet-name>
		<servlet-class>com.testproject.controller.user.UserUpdateServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserDeleteServlet</servlet-name>
		<servlet-class>com.testproject.controller.user.UserDeleteServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProjectReadServlet</servlet-name>
		<servlet-class>com.testproject.controller.project.ProjectReadServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProjectCreateServlet</servlet-name>
		<servlet-class>com.testproject.controller.project.ProjectCreateServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProjectUpdateServlet</servlet-name>
		<servlet-class>com.testproject.controller.project.ProjectUpdateServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProjectDeleteServlet</servlet-name>
		<servlet-class>com.testproject.controller.project.ProjectDeleteServlet</servlet-class>
	</servlet>




	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserReadServlet</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserCreateServlet</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserUpdateServlet</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserDeleteServlet</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProjectReadServlet</servlet-name>
		<url-pattern>/project</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProjectCreateServlet</servlet-name>
		<url-pattern>/project</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProjectUpdateServlet</servlet-name>
		<url-pattern>/project</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProjectDeleteServlet</servlet-name>
		<url-pattern>/project</url-pattern>
	</servlet-mapping>
</web-app>